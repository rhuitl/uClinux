config TARGET_ARCH
	string
	default "e1"

config FORCE_OPTIONS_FOR_ARCH
	bool
	default y
	select ARCH_BIG_ENDIAN
	select ARCH_HAS_NO_MMU
	select HAVE_NO_SHARED

config ARCH_E1 
	bool
	default y

config ARCH_CFLAGS
	string
	default "-mgnu-param"
